# Praval Manufacturing Analytics System - Environment Configuration
# Copy this file to .env and fill in your values

# OpenAI API Key (required for AI agents)
OPENAI_API_KEY=your-openai-api-key-here

# Optional: Override default model (default: gpt-4o-mini)
# OPENAI_MODEL=gpt-4o-mini

# Cube.js Configuration (defaults work with docker-compose)
CUBEJS_API_URL=http://localhost:4000/cubejs-api/v1
CUBEJS_API_SECRET=your-cubejs-secret

# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
